{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "todo/Dockerfile"
  },
  "deploy": {
    "startCommand": "java -jar -Dspring.profiles.active=railway target/todo-0.0.1-SNAPSHOT.jar",
    "healthcheckPath": "/api/actuator/health",
    "healthcheckTimeout": 300
  }
}